<h1>Listing analyses</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Title</th>
    <th>Body</th>
  </tr>

<% @analyses.each do |analysis| %>
  <tr>
    <td><%=h analysis.type %></td>
    <td><%=h analysis.title %></td>
    <td><%=h analysis.body %></td>
    <td><%= link_to 'Show', analysis %></td>
    <td><%= link_to 'Edit', edit_analysis_path(analysis) %></td>
    <td><%= link_to 'Destroy', analysis, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New analysis', new_analysis_path %>