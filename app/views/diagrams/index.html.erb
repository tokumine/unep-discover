<h1>Listing diagrams</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Name</th>
    <th>Image file name</th>
    <th>Image content type</th>
    <th>Image file size</th>
    <th>Image updated at</th>
  </tr>

<% @diagrams.each do |diagram| %>
  <tr>
    <td><%=h diagram.type %></td>
    <td><%=h diagram.name %></td>
    <td><%=h diagram.image_file_name %></td>
    <td><%=h diagram.image_content_type %></td>
    <td><%=h diagram.image_file_size %></td>
    <td><%=h diagram.image_updated_at %></td>
    <td><%= link_to 'Show', diagram %></td>
    <td><%= link_to 'Edit', edit_diagram_path(diagram) %></td>
    <td><%= link_to 'Destroy', diagram, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New diagram', new_diagram_path %>